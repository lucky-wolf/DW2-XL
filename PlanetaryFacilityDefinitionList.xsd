			<xs:element minOccurs="1" maxOccurs="1" name="PlanetaryFacilityDefinitionId" type="xs:short" />
			<xs:element minOccurs="0" maxOccurs="1" name="ItemBonuses" type="ArrayOfBonus" />
			<xs:element minOccurs="0" maxOccurs="1" name="EmpireBonuses" type="ArrayOfBonus" />
			<xs:element minOccurs="0" maxOccurs="1" name="BuildCost" type="Price" />
