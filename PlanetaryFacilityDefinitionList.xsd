<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="ArrayOfPlanetaryFacilityDefinition" nillable="true" type="ArrayOfPlanetaryFacilityDefinition" />
	<xs:complexType name="ArrayOfPlanetaryFacilityDefinition">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="unbounded" name="PlanetaryFacilityDefinition" nillable="true" type="PlanetaryFacilityDefinition" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PlanetaryFacilityDefinition">
