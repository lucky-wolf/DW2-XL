	<xs:complexType name="ArrayOfBonus">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="unbounded" name="Bonus" nillable="true" type="Bonus" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Bonus">
		<xs:all>
			<xs:element minOccurs="1" maxOccurs="1" name="Type" type="BonusType" />
