			<xs:element minOccurs="0" maxOccurs="1" name="Size" type="xs:float" />
			<xs:element minOccurs="1" maxOccurs="1" name="IsRuins" type="xs:boolean" />
			<xs:element minOccurs="1" maxOccurs="1" name="MaximumBuildCountPerGalaxy" type="xs:short" />
			<xs:element minOccurs="1" maxOccurs="1" name="MaximumBuildCountPerEmpire" type="xs:short" />
